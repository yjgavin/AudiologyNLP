<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextField?>

<AnchorPane prefHeight="550" prefWidth="700" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.com.controllers.NewDocController">

<children>
    <Label alignment="CENTER" layoutX="50.0" layoutY="100.0" prefHeight="36.0" prefWidth="150.0" text="New Document">
        <font>
            <Font size="18.0" />
        </font>
    </Label>

    <Button layoutX="70.0" layoutY="420.0" mnemonicParsing="false" onAction="#processHome" prefWidth="100.0" text="Back" />

    <Separator layoutX="225.0" layoutY="75.0" orientation="VERTICAL" prefHeight="400.0" prefWidth="0.0" />

    <Label layoutX="275.0" layoutY="235.0" prefWidth="170.0" text="Upload New Document:">
        <font>
            <Font size="14.0" />
        </font>
    </Label>

    <TextField fx:id="fileName" layoutX="275.0" layoutY="275.0" prefWidth="280.0" />

    <Button layoutX="550.0" layoutY="275.0" mnemonicParsing="false" onAction="#processLoadFile" prefWidth="80.0" text="Browse" />
    <Button layoutX="550.0" layoutY="420.0" mnemonicParsing="false" onAction="#processScan" prefWidth="80.0" text="Scan" />
</children>

</AnchorPane>
