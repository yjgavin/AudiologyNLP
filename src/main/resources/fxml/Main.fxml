<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="Profile" prefHeight="550" prefWidth="700" styleClass="profile" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.com.controllers.MainController">

    <children>
        <Label alignment="CENTER" layoutX="70.0" layoutY="100.0" prefHeight="36.0" prefWidth="100.0" text="Main Page">
            <font>
                <Font size="18.0" />
            </font>
        </Label>


        <Button layoutX="70.0" layoutY="380.0" mnemonicParsing="false" onAction="#processAddReport" prefWidth="100.0" text="Add Report" />
        <Button layoutX="70.0" layoutY="420.0" mnemonicParsing="false" onAction="#processLogout" prefWidth="100.0" text="Logout" />

        <Separator layoutX="225.0" layoutY="75.0" orientation="VERTICAL" prefHeight="400.0" prefWidth="0.0" />

        <TextField fx:id="searchBar" layoutX="250.0" layoutY="100.0" prefHeight="36.0" prefWidth="400.0" promptText="Search" text="Search By Patient" />
        <ListView fx:id="patientList" layoutX="250.0" layoutY="150.0" prefHeight="294.0" prefWidth="400.0" />
      <Button layoutX="70.0" layoutY="340.0" mnemonicParsing="false" onAction="#processTrainModel" prefWidth="100.0" text="Train Model" />
    </children>
</AnchorPane>
